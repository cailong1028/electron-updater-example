{
  "name": "electron-updater-example",
  "version": "0.5.18",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "publish_origin": "build -p always",
    "publish1": "build --dir -w",
    "publish2": "build --pd=./dist/win-unpacked",
    "electron-start": "set NODE_ENV=development&& electron --debug-brk=5000 ./main.js",
    "http": "http-server dist/ -p 8080",
    "http-evidence_bak": "http-server ../bqj-client-edition/release/ -p 8080",
    "http-evidence": "http-server ../nsis_setup_skin/Output/ -p 8080"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-builder": "^19.43.3"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^2.18.2",
    "http-server": "^0.11.1",
    "log4js": "^2.5.3"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
      }
    ],
    "appId": "com.github.iffy.electronupdaterexample",
    "mac": {
      "category": "your.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "output": "./dist"
    }
  }
}
